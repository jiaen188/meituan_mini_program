/**app.wxss**/
page {
  background: #f0f0f0;
  height: 100%;
}
.scroll-page {
  height: 100%;
}
.clear {
  display: table;
}
.clear::after {
  content: "";
  display: block;
  clear: both;
}
.navigator-hover {
  background: transparent;
}
.btn {
  width: 500rpx;
  margin: 20rpx auto;
  background: #f60 !important;
  color: #fff !important;
}